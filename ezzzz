getgenv().autoclick = false;

local library = loadstring(game:HttpGet(('https://raw.githubusercontent.com/AikaV3rm/UiLib/master/Lib.lua')))()

local w = library:CreateWindow("‡∏Ü‡∏ß‡∏¢") -- Creates the window

local b = w:CreateFolder("‡∏Ü‡∏ß‡∏¢") -- Creates the folder(U will put here your buttons,etc)

b:Label("‡∏£‡∏±‡∏Å‡∏û‡πà‡∏≠",{
    TextSize = 25; -- Self Explaining
    TextColor = Color3.fromRGB(255,255,255); -- Self Explaining
    BgColor = Color3.fromRGB(69,69,69); -- Self Explaining
    
}) 

b:Toggle("‡∏Å‡∏î‡πÅ‡∏•‡πâ‡∏ß‡πÄ‡∏õ‡πá‡∏ô‡∏Ñ‡∏ô‡∏£‡∏±‡∏Å‡∏ä‡∏≤‡∏îüò≠üò≠",function(bool)
    getgenv().autoclick = bool
    print("‡πÄ‡∏õ‡∏¥‡∏î‡∏•‡∏∞")
    if bool then
        dotap();
    end
end)

function dotap()
    spawn(function()
        while autoclick == true do
            workspace.Events.AddClick:FireServer()
            wait()
        end
    end)
end

print("it's working")
